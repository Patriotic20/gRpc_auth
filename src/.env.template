# DATABASE
APP_CONFIG__DB__URL=postgresql+asyncpg://<username>:<password>@<host>:<port>/<database_name>
APP_CONFIG__DB__ECHO=False
APP_CONFIG__DB__ECHO_POOL=False
APP_CONFIG__DB__POOL_SIZE=50
APP_CONFIG__DB__MAX_OVERFLOW=10


# JWT
APP_CONFIG__JWT__ACCESS_SECRET_KEY=your_access_secret
APP_CONFIG__JWT__REFRESH_SECRET_KEY=your_refresh_secret
APP_CONFIG__JWT__ACCESS_SECRET_MINUTES=15
APP_CONFIG__JWT__REFRESH_SECRET_DAY=7
APP_CONFIG__JWT__ALGORITHM=HS256



